<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sys="clr-namespace:System;assembly=netstandard"
             xmlns:conv="clr-namespace:XamarinFormsBooksSample.Converters"
             xmlns:ex="clr-namespace:XamarinFormsBooksSample.Extensions"
             xmlns:local="clr-namespace:XamarinFormsBooksSample"
             xmlns:views="clr-namespace:XamarinFormsBooksSample.Views"
             x:Class="XamarinFormsBooksSample.MainPage">

    <TabbedPage.Children>
        <NavigationPage Title="Books" x:Name="navigationPage">
            <x:Arguments>
                <views:BooksPage x:Name="booksPage" />
            </x:Arguments>
        </NavigationPage>
        <views:AboutPage Title="About" />
    </TabbedPage.Children>

</TabbedPage>
